{
  "name": "graphql-cms-back-sdk",
  "version": "1.0.0-527a4b85",
  "description": "GraphQL CMS - backend SDK",
  "main": "dist/index.js",
  "repository": "",
  "author": "Laegel",
  "license": "MIT",
  "scripts": {
    "generate": "gql-gen",
    "generate:watch": "nodemon --exec yarn generate -e graphql",
    "start:server": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:server:watch": "nodemon --exec yarn start:server -e ts",
    "dev": "concurrently \"yarn generate:watch\" \"yarn start:server:watch\"",
    "start": "yarn generate && yarn start:server",
    "build": "ttsc && yalc publish",
    "build:watch": "nodemon --exec tsc"
  },
  "dependencies": {
    "@graphql-modules/core": "0.6.1",
    "@types/express": "4.16.1",
    "@types/express-graphql": "0.6.2",
    "@types/graphql": "14.0.4",
    "bcryptjs": "2.4.3",
    "express": "4.16.4",
    "express-graphql": "0.7.1",
    "graphql": "^14.1.1",
    "graphql-tag": "2.10.1",
    "graphql-toolkit": "0.2.6",
    "graphql-tools": "4.0.4",
    "jsonwebtoken": "8.5.1",
    "module-alias": "2.2.0",
    "mongodb": "3.2.7",
    "reflect-metadata": "0.1.12",
    "ts-node": "7.0.1",
    "tsconfig-paths": "3.7.0",
    "type-graphql": "0.17.4",
    "typescript": "3.2.2"
  }
}
